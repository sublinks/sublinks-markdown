{
  "name": "sublinks-markdown",
  "version": "0.1.0",
  "description": "Markdown handling for sublinks frontends.\n\nHas a function called mdToHtml that can be called with the markdown you want to convert to html as one argument, and then the site domain (and protocol) as a second (e.g. https://programming.dev)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Ategon",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
